{
  "name": "asseambly-carving-v2",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "assemblyscript": "github:AssemblyScript/assemblyscript",
    "http-server": "^0.11.1",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --validate --debug --printrtti",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --validate --optimize",
    "asbuild:cp": "mkdir -p dist/wasm && cp build/* dist/wasm/",
    "asbuild": "run-s asbuild:untouched asbuild:cp",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "serve": "http-server dist -c-1",
    "start": "run-p build:watch serve"
  },
